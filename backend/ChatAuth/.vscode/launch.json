{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ChatAuth (uvicorn)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "index:app",
        "--host", "127.0.0.1",
        "--port", "1111",
        "--reload"
      ],
      "jinja": true,
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
